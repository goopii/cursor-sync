{
  "workbench.iconTheme": "material-icon-theme",
  "workbench.startupEditor": "none",
  "workbench.editorAssociations": {
    "*.ahk": "default"
  },
  "dart.flutterSdkPath": "C:\\Users\\malte\\OneDrive\\New folder\\flutter",
  "files.autoSave": "onFocusChange",
  "security.workspace.trust.untrustedFiles": "open",
  "security.workspace.trust.enabled": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "dart.devToolsBrowser": "default",
  "dart.debugExternalPackageLibraries": true,
  "dart.debugSdkLibraries": false,
  "git.confirmSync": false,
  "git.openRepositoryInParentFolders": "always",
  "git.autofetch": true,
  "npm.packageManager": "pnpm",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.preferences.importModuleSpecifier": "shortest",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnPaste": true,
  "editor.tabSize": 2,
  "[markdown]": {
    "editor.defaultFormatter": null
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.addMissingImports": "explicit"
  },
  "editor.wordWrap": "on",
  "editor.rulers": [80, 100],
  "editor.bracketPairColorization.enabled": true,
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "editor.suggestSelection": "first",
  "editor.linkedEditing": true,
  "workbench.editor.wrapTabs": true,
  "workbench.editor.pinnedTabsOnSeparateRow": true,
  "editor.guides.bracketPairs": true,
  "editor.scrollbar.horizontal": "visible",
  "editor.minimap.showSlider": "always",
  "editor.minimap.enabled": false,
  "editor.fastScrollSensitivity": 4,
  "files.associations": {
    "*.css": "tailwindcss",
    "*.md": "javascript"
  },
  "workbench.colorTheme": "Two Monokai",
  "tailwind-fold.autoFold": false,
  "tailwind-fold.unfoldIfLineSelected": true,
  "AHK++.v2.formatter": {
    "maxPreserveNewlines": 2,
    "wrapLineLength": 120,
    "arrayStyle": "collapse"
  },
  "AHK++.v2.warn": {
    "callWithoutParentheses": "On",
    "localSameAsGlobal": true
  },
  "vs-browser.url": "http://localhost:5173",
  "vs-browser.showUpdateChanges": false,
  "vs-browser.favourites.list": {
    "localhost": "http://localhost:5173"
  },
  "explorer.confirmPasteNative": false,
  "editor.formatOnSave": true,
  "window.commandCenter": true,
  "cursor.composer.textSizeScale": 0.85,
  "cursor.composer.shouldChimeAfterChatFinishes": true,
  "cursor.composer.backspaceRemoveContext": false,
  "window.newWindowProfile": "Default",
  "syncSettings.resources": [
    "extensions",
    "keybindings",
    "settings",
    "snippets",
    "uiState"
  ],
  "syncSettings.keybindingsPerPlatform": false
}
